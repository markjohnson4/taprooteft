@media screen and (max-width: var(--sm)) {
  .row-reverse-sm {
    flex-direction: row-reverse;
  }
}

.btn, .textInput, select {
  border-width: 2px;
  border-style: solid;
  padding: 0.5rem 1rem;
  border-radius: 0.5rem;
  line-height: 1.2;
  font-size: 1em;
}

.btn {
  display: inline-block;
  text-decoration: none;
}

.textInput, select {
  width: 100%;
  display: block;
}

.mobileMenuIcon {
  position: relative;
  display: block;
  margin: 0 auto;
  height: 1em;
  width: 1.2em;
}

.mobileMenuIcon span {
  display: block;
  position: absolute;
  left: 0;
  right: 0;
  height: 0.12em;
  background-color: currentColor;
}

.mobileMenuIcon span:first-child {
  top: 0.08em;
}

.mobileMenuIcon span:nth-child(2) {
  top: 50%;
  transform: translateY(-50%);
}

.mobileMenuIcon span:last-child {
  bottom: 0.08em;
}

body.is-active .mobileMenuIcon span:first-child {
  top: 50%;
  transform: translateY(-50%) rotate(45deg);
}

body.is-active .mobileMenuIcon span:nth-child(2) {
  background-color: transparent;
}

body.is-active .mobileMenuIcon span:last-child {
  top: 50%;
  transform: translateY(-50%) rotate(-45deg);
}

.block {
  display: block;
}

.none {
  display: none;
}

.inline {
  display: inline;
}

.inline-flex {
  display: inline-flex;
}

.width-iconNavLink {
  width: 2.5em;
}

svg {
  width: 1em;
  fill: currentColor;
  stroke: currentColor;
}

.full-width svg {
