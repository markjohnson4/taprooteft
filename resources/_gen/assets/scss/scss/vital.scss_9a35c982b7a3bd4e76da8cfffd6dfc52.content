.container {
  width: var(--container_width);
  max-width: 100%;
  margin: 0 auto;
  padding-left: var(--container_x_padding);
  padding-right: var(--container_x_padding);
}

body {
  overflow-x: hidden;
}

.containerOverride {
  width: 100vw;
  margin-left: 50%;
  transform: translateX(-50%);
}

@media (pointer: fine) {
  .containerOverride {
    width: calc(100vw - 12px);
  }
}

.flex {
