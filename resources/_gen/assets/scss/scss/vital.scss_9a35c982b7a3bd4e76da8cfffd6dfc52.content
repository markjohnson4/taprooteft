    font-size: var(--h4_size);
  }
}

h5 {
  font-weight: var(--h5_weight);
  line-height: var(--h5_line_height);
  font-size: calc(var(--h5_size) / var(--header_size_mobile_reduction));
}

@media (min-width: 768px) {
  h5 {
    font-size: var(--h5_size);
  }
}

h6 {
  font-weight: var(--h6_weight);
  line-height: var(--h6_line_height);
  font-size: calc(var(--h6_size) / var(--header_size_mobile_reduction));
}

@media (min-width: 768px) {
