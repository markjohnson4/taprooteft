}

.minisection, aside {
  border-style: solid;
  border-width: 5px;
  padding: 1rem;
}

aside {
  position: relative;
  margin-top: 3rem;
  border-radius: 10px;
  padding-top: 4rem;
}

aside.no-label {
  padding-top: 1rem;
}

aside.no-label:before {
  display: none;
}

aside:before {
  content: "Aside";
  position: absolute;
  top: 0;
  left: 0;
  border: 5px solid currentColor;
  border-top-width: 0;
  border-left-width: 0;
  padding: 0 0.5rem;
  border-radius: 0 0 10px 0;
  display: var(--aside_label_display);
  font-weight: bold;
}

@media (min-width: 768px) {
