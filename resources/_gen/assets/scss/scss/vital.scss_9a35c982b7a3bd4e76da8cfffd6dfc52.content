@charset "UTF-8";
body {
  margin: 0;
}

*,
*::before,
*::after {
  box-sizing: border-box;
  margin: 0;
}

textarea {
  resize: vertical;
}

table {
  border-collapse: collapse;
}

img,
video,
canvas,
audio,
iframe,
embed,
object {
  display: block;
}

img,
video {
  max-width: 100%;
  height: auto;
}

blockquote,
dl,
dd,
h1,
h2,
h3,
h4,
h5,
h6,
hr,
figure,
p,
pre {
  margin: 0;
}

html {
  scroll-behavior: smooth;
}

button {
  appearance: none;
  border: 0;
  padding: 0;
  margin: 0;
  background: none;
}

*:focus {
  outline-style: dashed;
  outline-width: 2px;
  outline-offset: 2px;
}

input,
select,
textarea {
  width: 100%;
  max-width: 100%;
  appearance: none;
}

input, select {
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}

.sr-only {
  position: absolute;
  width: 1px;
  height: 1px;
  padding: 0;
  margin: -1px;
  overflow: hidden;
  clip: rect(0, 0, 0, 0);
  white-space: nowrap;
  border-width: 0;
}

body {
  font-size: var(--default_font_size);
  line-height: var(--default_line_height);
  font-family: var(--font_family);
  color: var(--text_color);
  background-color: var(--background_color);
}

h1, h2, h3, h4, h5, h6 {
  color: var(--accent_color);
}

blockquote {
  border-left-color: var(--accent_color);
}

.minisection, aside {
  border-color: var(--accent_color);
}

aside:before {
  color: var(--accent_color);
}

.c-accent {
  color: var(--accent_color);
}

.bg-accent {
  background-color: var(--accent_color);
  color: var(--background_color);
}

a, .link {
  color: var(--action_color);
}

a:hover, a:focus, .link:hover, .link:focus {
  background-color: var(--action_color);
  color: var(--background_color);
}

.btn-primary {
  color: var(--background_color);
  background-color: var(--action_color);
  border-color: var(--action_color);
}

.btn-primary:hover, .btn-primary:focus {
  background-color: var(--text_color);
  color: var(--background_color);
}

*:focus {
  outline-color: var(--action_color);
}

h1, h2, h3, h4, h5, h6 {
  font-family: var(--header_font_family);
}

.bg-alt {
  border-color: var(--alt_border_color);
  color: var(--alt_text_color);
  background-color: var(--alt_background_color);
}

.bg-alt h1, .bg-alt h2, .bg-alt h3, .bg-alt h4, .bg-alt h5, .bg-alt h6 {
  color: var(--alt_accent_color);
}

.bg-alt blockquote {
  border-left-color: var(--alt_accent_color);
}

.bg-alt .minisection, .bg-alt aside {
  border-color: var(--alt_accent_color);
}

.bg-alt aside:before {
  color: var(--alt_accent_color);
}

.bg-alt .c-accent {
  color: var(--alt_accent_color);
}

.bg-alt .bg-accent {
  background-color: var(--alt_accent_color);
  color: var(--alt_background_color);
}

.bg-alt a, .bg-alt .link {
  color: var(--alt_action_color);
}

.bg-alt a:hover, .bg-alt a:focus, .bg-alt .link:hover, .bg-alt .link:focus {
  background-color: var(--alt_action_color);
  color: var(--alt_background_color);
}

.bg-alt .btn-primary {
  color: var(--alt_background_color);
  background-color: var(--alt_action_color);
  border-color: var(--alt_action_color);
}

.bg-alt .btn-primary:hover, .bg-alt .btn-primary:focus {
  background-color: var(--alt_text_color);
  color: var(--alt_background_color);
}

.bg-alt *:focus {
  outline-color: var(--alt_action_color);
}

h1 {
  font-weight: var(--h1_weight);
  line-height: var(--h1_line_height);
  font-size: calc(var(--h1_size) / var(--header_size_mobile_reduction));
}

@media (min-width: 768px) {
  h1 {
    font-size: var(--h1_size);
  }
}

h2 {
  font-weight: var(--h2_weight);
  line-height: var(--h2_line_height);
