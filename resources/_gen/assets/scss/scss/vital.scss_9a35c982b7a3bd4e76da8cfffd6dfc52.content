    font-size: var(--h6_size);
  }
}

.size-navLink {
  font-size: var(--navLinks_size);
}

.size-homeLink {
  font-size: var(--home_navLink_size);
}

.lead {
  font-size: calc(var(--leadText_size) / var(--header_size_mobile_reduction));
}

@media (min-width: 768px) {
  .lead {
    font-size: var(--leadText_size);
  }
}

picture {
  display: block;
}

p:empty {
  display: none;
}

.block, div, h1, h2, h3, h4, h5, h6, p, li, ul, ol, blockquote, audio, picture, figure {
  margin-top: 1.5em;
}

.inner-my-compact .block, .inner-my-compact div, .inner-my-compact h1, .inner-my-compact h2, .inner-my-compact h3, .inner-my-compact h4, .inner-my-compact h5, .inner-my-compact h6, .inner-my-compact p, .inner-my-compact li, .inner-my-compact ul, .inner-my-compact ol, .inner-my-compact blockquote, .inner-my-compact audio, .inner-my-compact picture, .inner-my-compact figure {
